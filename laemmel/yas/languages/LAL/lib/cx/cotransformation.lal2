language Any1
language L1 <= Any1
language Any2
language L2 <= Any2
relation consistent : L1 # L2
language XAny
language XL <= XAny
function interpret : XL # L1 ~> L1
function interpret : XL # L2 ~> L2
axiom consistency {
 forall t <- XL.
  forall a <- L1.
   forall c <- L1.
    forall b <- L2.
     forall d <- L2.
      ((consistent(a, b)) /\ ((interpret(t, a) = c) /\ (interpret(t, b) = d))) => (consistent(c, d))
}
