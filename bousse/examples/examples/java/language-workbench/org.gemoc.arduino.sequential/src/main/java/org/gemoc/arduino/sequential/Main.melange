package org.gemoc.arduino.sequential

language Arduino {
	
	/*
	 * Declare abstract syntax
	 */
	syntax "platform:/resource/org.gemoc.arduino.sequential.model/model/arduino.ecore"
	
	/*
	 * Declare DSE
	 */
//	with org.gemoc.arduino.operationalsemantics.*
	
	/*
	 * Set name of the ModelType (ie: the type of this language)
	 */
	exactType ArduinoMT
}

language ExecArduino inherits Arduino {
	with org.gemoc.arduino.sequential.operationalsemantics.*
	exactType ExecArduinoMT
}
